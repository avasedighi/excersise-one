class Airline:
  def __init__(self, airlineName, callSign, airlineType, phoneNumber):
    self.airlineName = airlineName
    self.callSign = callSign
    self.airlineType = airlineType
    self.phoneNumber = str(phoneNumber)

a1 = Airline('IranAir', 'AiranAir', 'International', '021 4662 1888')
